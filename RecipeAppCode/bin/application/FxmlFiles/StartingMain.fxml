<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="startingMainPane" prefHeight="450.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StartingMainController">
   <left>
      <AnchorPane prefHeight="450.0" prefWidth="325.0" styleClass="appPanel" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="63.0" layoutY="150.0" smooth="false">
               <image>
                  <Image url="@../Photos/AppLogo.png" />
               </image>
            </ImageView>
         </children></AnchorPane>
   </left>
   <center>
      <VBox alignment="CENTER" minHeight="450.0" minWidth="325.0" prefHeight="450.0" prefWidth="325.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#openLoginPage" prefHeight="33.0" prefWidth="77.0" styleClass="submitButton" text="Login">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Button>
            <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#openSignUpPage" prefHeight="32.0" prefWidth="95.0" styleClass="submitButton" text="Sign Up">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </VBox.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
