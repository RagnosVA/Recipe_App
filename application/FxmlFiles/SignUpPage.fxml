<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="450.0" prefWidth="325.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SignUpPageController">
   <children>
      <TextField fx:id="firstNameInput" alignment="CENTER" maxHeight="25.0" maxWidth="225.0" promptText="first name" styleClass="userInput">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="lastNameInput" alignment="CENTER" layoutX="60.0" layoutY="196.0" maxHeight="25.0" maxWidth="225.0" promptText="last name" styleClass="userInput">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="emailInput" alignment="CENTER" layoutX="60.0" layoutY="271.0" maxHeight="25.0" maxWidth="225.0" promptText="e-mail" styleClass="userInput">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TextField>
      <TextField fx:id="passwordInput" alignment="CENTER" layoutX="60.0" layoutY="285.0" maxHeight="25.0" maxWidth="225.0" promptText="password" styleClass="userInput">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TextField>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#userSignUp" prefHeight="31.0" prefWidth="79.0" styleClass="submitButton" text="Sign Up">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Label fx:id="errorLabel" text="Label" textFill="RED" visible="false" />
   </children>
</VBox>
